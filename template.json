{
  "name": "{theme-name}",
  "repositories": [
    {
      "repoType": "{homepage-github-bitbucket-gitlab}",
      "repoUrl": "{url-of-repo-project}"
    }
  ],
  "require": {
    "{prefix_plugin-name}": "{semver}"
  },
  "stable": {
    "version": "{semver}",
    "datePublished": "{yyyy}-{mm}-{dd}",
    "downloadUrl": "{url-of-theme-download}",
    "demoUrl": "{url-of-theme-demo}"
  },
  "legacy": [
    {
      "4.7": {
        "version": "{semver}",
        "datePublished": "{yyyy}-{mm}-{dd}",
        "downloadUrl": "{url-of-theme-download}",
        "demoUrl": "{url-of-theme-demo}"
      }
    }
  ]
}
